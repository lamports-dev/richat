{
  "libpath": "../target/release/librichat_plugin.so",
  "log": {
    "level": "info"
  },
  "tokio": {
    "worker_threads": 4,
    "affinity": "0-3"
  },
  "channel": {
    "max_messages": "2_097_152",
    "max_slots": 100,
    "max_bytes": "10_737_418_240"
  },
  "quic": {
    "endpoint": "0.0.0.0:10100",
    "tls_config": {
      // "cert": "/path/to/cert.cert",
      // "key": "/path/to/key.key",
      "self_signed_alt_names": ["localhost"]
    },
    "expected_rtt": 100,
    "max_stream_bandwidth": 12500000,
    "max_recv_streams": 16
  },
  "tcp": {
    "endpoint": "0.0.0.0:10101",
    "backlog": 1024,
    "keepalive": null,
    "nodelay": true,
    "send_buffer_size": null
  },
  "grpc": {
    "endpoint": "0.0.0.0:10102",
    "tls_config": {
      "cert": "/path/to/cert.cert",
      "key": "/path/to/key.key"
    },
    "compression": {
      "accept": ["gzip", "zstd"],
      "send": ["gzip", "zstd"]
    },
    "max_decoding_message_size": "4_194_304",
    "server_tcp_keepalive": "20s",
    "server_tcp_nodelay": true,
    "server_http2_adaptive_window": null,
    "server_http2_keepalive_interval": null,
    "server_http2_keepalive_timeout": null,
    "server_initial_connection_window_size": null,
    "server_initial_stream_window_size": null
  },
  "prometheus": {
    "endpoint": "0.0.0.0:10123"
  }
}
